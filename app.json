{
  "expo": {
    "name": "flickr-fun",
    "description": "A fun sample project using React Native/Redux",
    "slug": "flickr-fun",
    "privacy": "public",
    "sdkVersion": "27.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "co.alius.flickrFun"
    },
    "android": {
      "package": "co.alius.flickrFun"
    }
  }
}
